<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/layoutComplete"
                android:paddingLeft="@dimen/margin_content_input_tablet"
                android:paddingRight="@dimen/margin_content_input_tablet">
		<ImageView
				android:id="@+id/icon"
				android:layout_width="@dimen/width_img_complete_confirm_apply_tablet"
				android:layout_height="@dimen/height_img_complete_confirm_apply_tablet"
				android:background="@drawable/ic_approval_confirmation"
				android:layout_centerHorizontal="true"
		        android:layout_marginTop="@dimen/margin_top_img_complete_confirm_apply_tablet"
				/>
		<TextView
				android:id="@+id/titleSuccess"
				android:text="@string/approval_completed"
				android:layout_centerHorizontal="true"
				android:textSize="@dimen/text_size_title"
				android:textColor="@color/color_title_input"
				android:layout_marginTop="@dimen/margin_top_title_complete_confirm_apply_tablet"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"/>
		<Button
					android:id="@+id/btnStartUsing"
					android:text="@string/beginning_to_use_procedures"
					android:layout_gravity="center_horizontal"
					android:textColor="@color/text_color_active"
					android:background="@android:color/transparent"
					android:textSize="@dimen/text_size_title"
					android:onClick="clickStart"
					android:textAllCaps="false"
					android:layout_centerHorizontal="true"
					android:layout_below="@+id/icon"
					android:layout_marginTop="@dimen/margin_top_button_complete_confirm_apply_tablet"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"/>
</RelativeLayout>