<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background">
    <include layout="@layout/layout_header"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="10dp"
        android:paddingTop="25dp"
        android:paddingLeft="20dp"
        android:textSize="17sp"
        android:background="#ddebfc"
        android:textAllCaps="true"
        android:text="@string/label_list_certificate"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/line_cert"/>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/background_color_white">
    <RelativeLayout
        android:id="@+id/menuSettingCertList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:clickable="true"
        android:background="@color/background_color_white">
        <Button
            android:id="@+id/btnSettingListCert"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:background="@android:color/transparent"
            android:layout_toLeftOf="@+id/ivBtnSetting1End"
            android:paddingBottom="10dp"
            android:textAllCaps="false"
            android:paddingTop="10dp"
            android:paddingLeft="20dp"
            android:paddingRight="10dp"
            android:textSize="17sp"
            android:gravity="left|center_vertical"
            android:text="@string/list_cert"/>
        <ImageView
            android:id="@+id/ivBtnSetting1End"
            android:layout_width="9dp"
            android:layout_height="15dp"
            android:src="@drawable/ic_arrow"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="20dp"
        android:background="@color/line_cert"/>
    <RelativeLayout
        android:id="@+id/menuSettingNotif"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:clickable="true"
        android:background="@color/background_color_white">
        <Button
            android:id="@+id/btnSettingNotification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:background="@android:color/transparent"
            android:layout_toLeftOf="@+id/ivBtnSetting2End"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:paddingLeft="20dp"
            android:paddingRight="10dp"
            android:textSize="17sp"
            android:textAllCaps="false"
            android:gravity="left|center_vertical"
            android:text="@string/notif_setting"/>
        <ImageView
            android:id="@+id/ivBtnSetting2End"
            android:layout_width="9dp"
            android:layout_height="15dp"
            android:src="@drawable/ic_arrow"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            />
    </RelativeLayout>
    </LinearLayout>
	<LinearLayout
			android:id="@+id/ll_mdm"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical">
		<TextView
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/line_cert"/>

		<TextView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:paddingBottom="10dp"
				android:paddingTop="25dp"
				android:paddingLeft="20dp"
				android:textSize="17sp"
				android:textAllCaps="true"
				android:text="@string/mdm"/>
		<TextView
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/line_cert"/>
		<RelativeLayout
				android:id="@+id/menuSettingMDM"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:clickable="true"
				android:background="@color/background_color_white">
			<Button
					android:id="@+id/btnSettingMDM"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:clickable="false"
					android:background="@android:color/transparent"
					android:layout_toLeftOf="@+id/ivBtnSettingMDM"
					android:paddingBottom="10dp"
					android:paddingTop="10dp"
					android:paddingLeft="20dp"
					android:paddingRight="10dp"
					android:textSize="17sp"
					android:textAllCaps="false"
					android:gravity="left|center_vertical"
					android:text="@string/profile"/>
			<ImageView
					android:id="@+id/ivBtnSettingMDM"
					android:layout_width="9dp"
					android:layout_height="15dp"
					android:src="@drawable/ic_arrow"
					android:layout_centerVertical="true"
					android:layout_alignParentRight="true"
					android:layout_marginRight="20dp"
					/>
		</RelativeLayout>
	</LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/line_cert"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="10dp"
        android:paddingTop="25dp"
        android:paddingLeft="20dp"
        android:textSize="17sp"
        android:textAllCaps="true"
        android:text="@string/label_other_setting"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/line_cert"/>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/background_color_white">
    <RelativeLayout
        android:id="@+id/menuSettingProduct"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:clickable="true"
        android:background="@color/background_color_white">
        <Button
            android:id="@+id/btnSettingProductInfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:background="@android:color/transparent"
            android:layout_toLeftOf="@+id/ivBtnSetting3End"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:paddingLeft="20dp"
            android:paddingRight="10dp"
            android:textSize="17sp"
            android:textAllCaps="false"
            android:gravity="left|center_vertical"
            android:text="@string/label_product_setting"/>
        <ImageView
            android:id="@+id/ivBtnSetting3End"
            android:layout_width="9dp"
            android:layout_height="15dp"
            android:src="@drawable/ic_arrow"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="20dp"
        android:background="@color/line_cert"/>
    <RelativeLayout
        android:id="@+id/menuSettingLibrary"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:clickable="true"
        android:background="@color/background_color_white">
        <Button
            android:id="@+id/btnSettingLibrary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:background="@android:color/transparent"
            android:layout_toLeftOf="@+id/ivBtnSetting4End"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:paddingLeft="20dp"
            android:paddingRight="10dp"
            android:textSize="17sp"
            android:textAllCaps="false"
            android:gravity="left|center_vertical"
            android:text="@string/label_library_setting"/>
        <ImageView
            android:id="@+id/ivBtnSetting4End"
            android:layout_width="9dp"
            android:layout_height="15dp"
            android:src="@drawable/ic_arrow"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            />
    </RelativeLayout>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/line_cert"/>
</LinearLayout>